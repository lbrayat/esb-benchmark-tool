<scenario xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="scenario.xsd">
    <configuration>
        <actor identifier="1">
            <type>consumer</type>
            <address>http://localhost:9080/CompositeApplication/consumerPort?wsdl</address>
        </actor>
        <actor identifier="51">
            <type>provider</type>
            <address>http://localhost:9080/CompositeApplication/providerPort?wsdl</address>
        </actor>
    </configuration>
    <consumers>
        <consumer identifier="1">
                <phaseConsumer>
                    <providerId>51</providerId>
		    <targetedProviderConf>2</targetedProviderConf>
                    <trafficClass>BE</trafficClass>
                    <numberOfRequests>250</numberOfRequests>
                    <sendPeriod>10</sendPeriod>
                    <packetSize>2</packetSize>
                    <startDate>10</startDate>
                    <numberOfBursts>1</numberOfBursts>
                </phaseConsumer>
                <phaseConsumer>
                    <providerId>51</providerId>
		    <targetedProviderConf>1</targetedProviderConf>
                    <trafficClass>RT</trafficClass>
                    <numberOfRequests>2000</numberOfRequests>
                    <sendPeriod>10</sendPeriod>
                    <packetSize>3</packetSize>
                    <startDate>30</startDate>
                    <numberOfBursts>1</numberOfBursts>
                </phaseConsumer>
        </consumer>
    </consumers>
    <providers>
        <provider identifier="51">
            <phaseProvider id="1">
                <processingTime>60</processingTime>
                <packetSize>5</packetSize>
                <percentageLoss>0</percentageLoss>
            </phaseProvider> 
            <phaseProvider id="2">
                <processingTime>2</processingTime>
                <packetSize>10</packetSize>
                <percentageLoss>0</percentageLoss>
            </phaseProvider>
        </provider>
    </providers>
</scenario>
