<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="CompositeApp" xmlns:ns2="http://beta/" xmlns:ns3="http://providerPckg/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>CompositeApp</name>
            <description>Represents the Service Assembly of CompositeApp</description>
        </identification>
        <service-unit>
            <identification>
                <name>CompositeApp-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="consumerPort" service-name="ns1:CompositeAppService1"/>
                <provider endpoint-name="ConsumerWebServicePort" service-name="ns2:ConsumerWebServiceService"/>
            </connection>
            <connection>
                <consumer endpoint-name="providerPort" service-name="ns1:CompositeAppService2"/>
                <provider endpoint-name="ProviderWSPort" service-name="ns3:ProviderWSService"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
