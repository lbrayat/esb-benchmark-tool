<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-ProviderWSService_2" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/ProviderWSService_2"/>
        <property name="wsdl-ProviderWSService_2" location="xml-resources/web-service-references/ProviderWSService_2/wsdl/localhost_8080/ProviderProject/ProviderWSService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/ProviderWSService_2" destdir="${build.generated.dir}/jax-wsCache/ProviderWSService_2" wsdl="${wsdl-ProviderWSService_2}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/ProviderProject/ProviderWSService?wsdl" xnocompile="true">
            <depends file="${wsdl-ProviderWSService_2}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/ProviderWSService_2"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/ProviderWSService_2">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-ProviderWSService_2" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/ProviderWSService_2"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/providerpckg"/>
    </target>
    <target name="wsimport-client-ConsumerWebServiceService_2" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/ConsumerWebServiceService_2"/>
        <property name="wsdl-ConsumerWebServiceService_2" location="xml-resources/web-service-references/ConsumerWebServiceService_2/wsdl/localhost_8080/ConsumerProject/ConsumerWebServiceService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/ConsumerWebServiceService_2" destdir="${build.generated.dir}/jax-wsCache/ConsumerWebServiceService_2" wsdl="${wsdl-ConsumerWebServiceService_2}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/ConsumerProject/ConsumerWebServiceService?wsdl" xnocompile="true">
            <depends file="${wsdl-ConsumerWebServiceService_2}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/ConsumerWebServiceService_2"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/ConsumerWebServiceService_2">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-ConsumerWebServiceService_2" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/ConsumerWebServiceService_2"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/beta"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-ProviderWSService_2, wsimport-client-ConsumerWebServiceService_2"/>
</project>
